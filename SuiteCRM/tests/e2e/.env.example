# E2E Test Environment Configuration
# Copy this file to .env and update with your values

# Database connection for test data setup
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=root
DB_NAME=suitecrm
DB_PORT=3306

# SuiteCRM URL
SUITECRM_URL=http://localhost:8080

# Test user credentials
TEST_USER=admin
TEST_PASSWORD=admin

# Visual regression settings
UPDATE_BASELINES=false

# Test execution settings
HEADLESS=true
SLOW_MO=0
TIMEOUT=30000

# Parallel execution
WORKERS=1

# Screenshot settings
SCREENSHOTS_ON_FAILURE=true
VIDEO_ON_FAILURE=true

# Test data prefix (to identify test records)
TEST_DATA_PREFIX=E2E_TEST_